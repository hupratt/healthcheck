{
    "java.configuration.updateBuildConfiguration": "interactive",
    "editor.formatOnSave": true,
    "[java]": {
        "editor.defaultFormatter": "redhat.java"
    },
    "gitPrefix.replacement #1": "$1: Adding the", // $1: Added
    "gitPrefix.replacement #2": "$1: Refactoring the", // $1: Updated
    "gitPrefix.replacement #3": "$1: Fixing the", // $1: Fixed
    "gitPrefix.replacement #4": "$1: Minor style changes to", // $1: Removed
}
